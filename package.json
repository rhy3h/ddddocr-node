{
  "name": "ddddocr-node",
  "version": "2.1.0",
  "description": "The JS version of DdddOcr",
  "homepage": "https://github.com/rhy3h/ddddocr-node#readme",
  "bugs": {
    "url": "https://github.com/rhy3h/ddddocr-node/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhy3h/ddddocr-node.git"
  },
  "license": "MIT",
  "keywords": [
    "ocr",
    "ddddocr"
  ],
  "author": "rhy3h",
  "files": [
    "dist",
    "onnx"
  ],
  "scripts": {
    "prepare": "tshy",
    "pretest": "npm run prepare",
    "test": "vitest run",
    "test:watch": "vitest",
    "tsdoc": "npx typedoc src/index.ts --out public"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.22.0",
    "await-to-js": "^3.0.0",
    "jimp": "^1.6.0",
    "onnxruntime-node": "1.15",
    "onnxruntime-web": "^1.23.2"
  },
  "devDependencies": {
    "@types/node": "^25.3.1",
    "rimraf": "^6.0.1",
    "tshy": "^3.1.0",
    "typedoc": "^0.27.6",
    "typescript": "^5.7.3",
    "vitest": "^4.0.18"
  },
  "tshy": {
    "exclude": [
      "**/*.test.ts"
    ],
    "esmDialects": [
      "browser"
    ],
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts"
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "browser": {
        "types": "./dist/browser/index.d.ts",
        "default": "./dist/browser/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    }
  },
  "main": "./dist/commonjs/index.js",
  "typings": "dist/commonjs/index.d.ts",
  "type": "module",
  "types": "./dist/commonjs/index.d.ts",
  "module": "./dist/esm/index.js"
}
